{
	"useTabs": true,
	"tabWidth": 4,
	"singleQuote": true,
	"printWidth": 120,
	"trailingComma": "all",
	"importOrder": ["^next|^react|next$|react$/(.*)$", "^@", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": true,
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "auto"
}
